/**
 *  Exclusive-or gate: out = !(a == b).
 */

CHIP Xor {
    IN a, b;
    OUT out;

    PARTS:
    // Bens Xor gate
    And(a=a, b=b, out=out1);
    Not(in=a, out=nota);
    Not(in=b, out=notb);    
    And(a=nota, b=notb, out=out2);
    Or(a=out1, b=out2, out=out3);
    Not(in=out3, out=out);
}
